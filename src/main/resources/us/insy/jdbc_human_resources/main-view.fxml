<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="426.0" prefWidth="405.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="us.insy.jdbc_human_resources.MainController">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="View">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#onMenuViewOpen" text="Open" />
                  <MenuItem mnemonicParsing="false" onAction="#onMenuClose" text="Close" />
               </items></Menu>
          <Menu mnemonicParsing="false" onAction="#onMenuEditClicked" text="Edit">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#onMenuEditOpen" text="Open" />
                  <MenuItem mnemonicParsing="false" onAction="#onMenuClose" text="Close" />
               </items></Menu>
          <Menu mnemonicParsing="false" onAction="#onMenuAddClicked" text="Add">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#onMenuAddOpen" text="Open" />
                  <MenuItem mnemonicParsing="false" onAction="#onMenuClose" text="Close" />
               </items></Menu>
        </menus>
      </MenuBar>
      <HBox fx:id="hBoxMainArea" maxWidth="1.7976931348623157E308" prefHeight="0.0" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
